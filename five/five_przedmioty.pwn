							}
						}
						PojazdInfo[uidv][pAudioStream] = 0;
						cmd_fasdasfdfive(playerid, "wyciaga p³ytke z systemu audio.");
					}
					else
					{
						new audio[128];
						format(audio, sizeof(audio), "%s",PrzedmiotInfo[uid][pWar3]);
						PojazdInfo[uidv][pAudioStream] = uid;
						ForeachEx(i, IloscGraczy)
