	return find;
}
forward DodajPrzedmiot(owner, typwlas, typ, war1, war2, name[], stworzyciel, waga, termin, war4, obj, cenas, tresc[]);
public DodajPrzedmiot(owner, typwlas, typ, war1, war2, name[], stworzyciel, waga, termin, war4, obj, cenas, tresc[])
{
	if(typ == P_ZARCIE)
	{
		termin = gettime() + (86400*2);
	}
	new index = GetFreeSQLUID("five_przedmioty", "ID");
	format(zapyt, sizeof(zapyt), "INSERT INTO `five_przedmioty` (`ID`, `WLASCICIEL`, `TYP_WLASCICIELA`, `TYP`, `WARTOSC_1`, `WARTOSC_2`, `UID_STWORZYCIEL`, `NAZWA`, `WAGA`, `TERMIN`, `CENAS`, `WARTOSC_3_tekst_global`, `OBJECT`) VALUES ('%d', '%d', '%d', '%d', '%d', '%d', '%d', '%s', '%f', '%d', '%d', '%s', '%d')",
	 index, owner, typwlas, typ, war1, war2, stworzyciel, name, waga, termin, cenas, tresc, obj);
	mysql_check();
	mysql_query2(zapyt);
	//index = mysql_insert_id();
	PrzedmiotInfo[index][pUID] = index;
	PrzedmiotInfo[index][pOwner] = owner;
	PrzedmiotInfo[index][pTypWlas] = typwlas;
	PrzedmiotInfo[index][pTyp] = typ;
	PrzedmiotInfo[index][pWar1] = war1;
	PrzedmiotInfo[index][pWar2] = war2;
	format(PrzedmiotInfo[index][pWar3], 124, "%s", tresc);
	PrzedmiotInfo[index][pWar4] = war4;
	PrzedmiotInfo[index][pStworzylUID] = stworzyciel;
	PrzedmiotInfo[index][pWaga] = waga;
	PrzedmiotInfo[index][pTermin] = termin;
	PrzedmiotInfo[index][pObject] = obj;
	PrzedmiotInfo[index][pCenas] = cenas;
	format(PrzedmiotInfo[index][pNazwa], 124, "%s", name);
	mysql_free_result();
	ZapiszPrzedmiot(index);
	return index;
}