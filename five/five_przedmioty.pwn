forward UzywanieItemu(playerid, uid);
public UzywanieItemu(playerid, uid)
{
	new typ = PrzedmiotInfo[uid][pTyp];
	RefreshNick(playerid);
	if(PrzedmiotInfo[uid][pOwner] != DaneGracza[playerid][gUID] && PrzedmiotInfo[uid][pTypWlas] != TYP_WLASCICIEL)
	{
	    dShowPlayerDialog(playerid, DIALOG_INFO, DIALOG_STYLE_MSGBOX, "{88b711}» {FFFFFF}"VER" {88b711}» {FFFFFF}Informacja{88b711}:", "{DEDEDE}Ten przedmiot {88b711}nie{DEDEDE} nale¿y do ciebie.", "Zamknij", "");
	    return 1;
	}
	if(typ == P_WEAPON)
	{
		if(IsPlayerInAnyVehicle(playerid))
		{
			dShowPlayerDialog(playerid, DIALOG_INFO, DIALOG_STYLE_MSGBOX, "{88b711}» {FFFFFF}"VER" {88b711}» {FFFFFF}Informacja{88b711}:", "{DEDEDE}Nie mo¿esz {88b711}u¿ywaæ{DEDEDE} broni, gdy jesteœ w pojezdzie!", "Zamknij", "");
		    return 0;
		}
	    new find = SprawdzCzyTrzymaTakaBron(playerid, PrzedmiotInfo[uid][pWar1]);
	    if(find > 0 && PrzedmiotInfo[uid][pUzywany] == 0)
		{
			dShowPlayerDialog(playerid, DIALOG_INFO, DIALOG_STYLE_MSGBOX, "{88b711}» {FFFFFF}"VER" {88b711}» {FFFFFF}Informacja{88b711}:", "{DEDEDE}Trzymasz ju¿ w {88b711}rêca{DEDEDE} tak¹ broñ.", "Zamknij", "");
			return 1;
		}
		if((PrzedmiotInfo[uid][pWar1] >= 1 && PrzedmiotInfo[uid][pWar1] <= 15) || PrzedmiotInfo[uid][pWar1] == 40 || PrzedmiotInfo[uid][pWar1] == 44 || PrzedmiotInfo[uid][pWar1] == 45 || PrzedmiotInfo[uid][pWar1] == 46)
		{
		    if(PrzedmiotInfo[uid][pWar2] <= 0) PrzedmiotInfo[uid][pWar2] = 10;
		}
