	    	//new metekst_global[124];
	    	//format(metekst_global, sizeof(metekst_global), "* %s odk³ada coœ na pod³oge.", ZmianaNicku(playerid));
			//SendWrappedMessageToPlayerRange(playerid, FIOLETOWY, metekst_global, 10);
			ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 4.1, 0, 0, 0, 0, 0);
			Transakcja(T_IODLOZ, DaneGracza[playerid][gUID], -1, DaneGracza[playerid][gGUID], -1, -1, uid, PrzedmiotInfo[uid][pWar1], -1, "-", gettime()-CZAS_LETNI);
	    }
	}else{
	    dShowPlayerDialog(playerid, DIALOG_INFO, DIALOG_STYLE_MSGBOX, "{88b711}» {FFFFFF}"VER" {88b711}» {FFFFFF}Informacja{88b711}:", "{DEDEDE}Ten przedmot {88b711}aktualnie{DEDEDE} jest u¿ywany.", "Zamknij", "");
